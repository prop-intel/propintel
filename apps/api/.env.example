# Database
DATABASE_URL="postgresql://postgres:password@localhost:5432/propintel"

# Required API Keys
# NOTE: Required for AI features. Use "sk-placeholder" for local dev if not running AI tasks.
OPENAI_API_KEY="sk-placeholder"

# Langfuse Observability (Optional)
# LANGFUSE_PUBLIC_KEY=
# LANGFUSE_SECRET_KEY=
# LANGFUSE_BASE_URL=https://us.cloud.langfuse.com

# Tavily API (Optional)
# TAVILY_API_KEY=

# AWS Configuration
# S3_BUCKET: When set, files are stored in S3. When empty/unset, uses local .local-storage/ directory.
# USE_LOCAL_STORAGE: Set to "true" to force local storage even when S3_BUCKET is configured.
AWS_REGION=us-west-2
AWS_PROFILE=default
S3_BUCKET=your-bucket-name
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=

# Local Development (Windows SSL fix)
NODE_TLS_REJECT_UNAUTHORIZED=0